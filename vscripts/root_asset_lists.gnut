void function ShLoadWeaponAssetsInit()
{
    WEAPON_ASSETS.clear()

    var dataTable = GetDataTable( $"datatable/weapon_itemflavor_sdk.rpak" )
    int numRows = GetDatatableRowCount( dataTable )
    int weaponColumn = GetDataTableColumnByName( dataTable, "weaponflavor" )

    for ( int i = 0; i < numRows; i++ )
    {
        string assetPathString = GetDataTableString( dataTable, i, weaponColumn )
        asset assetPath = GetAssetFromString( assetPathString )
        WEAPON_ASSETS.append( assetPath )
    }
}