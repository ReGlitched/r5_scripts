global function SV_CL_Shared_Init

void function SV_CL_Shared_Init()
{
#if SERVER
	SvDemo_ConsistencyCheckString( "SV_CL_Shared_Init()" )
#endif

	PlayLists_Mapnames_Gamemodes_Init()
	Initialize_SH_FlowstateSettings()
	
	GameModes_Init()

	if ( IsLobby() )
	{
		Shared_Lobby_Init()
		return
	}

	SV_CL_Init()

	#if SERVER
		Server_Init()
	#endif

	#if CLIENT
		Client_Init()
	#endif

#if SERVER
	SvDemo_ConsistencyCheckString( "SV_CL_Shared_Init END" )
#endif
}


